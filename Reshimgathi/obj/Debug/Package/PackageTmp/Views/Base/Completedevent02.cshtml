    <script type="text/javascript">
        function loadMemories(file, callback) {
            var rawFile = new XMLHttpRequest();
            rawFile.overrideMimeType("application/json");
            rawFile.open("GET", file, true);
            rawFile.onreadystatechange = function () {
                if (rawFile.readyState === 4 && rawFile.status == "200") {
                    callback(rawFile.responseText);
                }
            }
            rawFile.send(null);
        }

        //usage:
        loadMemories("/images/json/yHmB1RW557qaXnzF8.json", function (text) {
            var parent = null;
            var data = JSON.parse(text);
            //console.log(data);
            var photoGridForm = "";
            for (var i = 0; i < data.Photos.length; i = i + 3) {
                photoGridForm = "<div class='gallery-grids'>";

                photoGridForm += "<div class='col-md-4 gallery-grid'><div class='gallery-grd'><a class='swipebox' href='" + data.Photos[i].URL + "'><img src='" + data.Photos[i].URL + "' class='img-style row6' alt=''><span class='zoom-icon'></span></a></div></div>";

                photoGridForm += "<div class='col-md-4 gallery-grid'><div class='gallery-grd'><a class='swipebox' href='" + data.Photos[i + 1].URL + "'><img src='" + data.Photos[i + 1].URL + "' class='img-style row6' alt=''><span class='zoom-icon'></span></a></div></div>";

                photoGridForm += "<div class='col-md-4 gallery-grid'><div class='gallery-grd'><a class='swipebox' href='" + data.Photos[i + 2].URL + "'><img src='" + data.Photos[i + 2].URL + "' class='img-style row6' alt=''><span class='zoom-icon'></span></a></div></div>";

                photoGridForm += "<div class='clearfix'></div></div>";

                parent = document.getElementById('photoscontainer');
                parent.insertAdjacentHTML('beforeend', photoGridForm);//child added.
            }
            $(".swipebox").swipebox();
            console.log("done");
        });

    </script>
